    "@types/yup": "^0.26.23",
    "assets-webpack-plugin": "^3.9.5",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-preval": "^3.0.1",
    "babel-plugin-source-map-support": "^2.0.1",
