{
  "name": "manage-frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon --inspect ./dist/server",
    "type-check": "tsc --noEmit --skipLibCheck",
    "pretty-quick": "pretty-quick",
    "lint": "tslint -p .",
    "bundle": "copy-node-modules . ./dist && yarn webpack --config webpack.production.js",
    "bundle-dev": "yarn bundle-dev-server && yarn bundle-dev-client",
    "bundle-dev-server": "webpack --config webpack.dev.server.js",
    "bundle-dev-client": "webpack --config webpack.dev.client.js",
    "bundle-check-client": "webpack --config webpack.prod.client.bundle-check.js",
    "bundle-check-client-if-changed": "((git diff --name-only --cached | grep 'yarn.lock') && yarn bundle-check-client) || true",
    "serve-dev": "webpack-dev-server --config ./webpack.dev.client.js --public manage.thegulocal.com",
    "watch": "yarn bundle-dev-server && (yarn bundle-dev-server -w & yarn start & yarn serve-dev)",
    "test": "snyk test && jest --coverage && yarn type-check && yarn lint ",
    "updateSnapshot": "jest -u",
    "sonar-scanner": "cd .. && app/node_modules/sonar-scanner/bin/sonar-scanner -Dsonar.projectVersion=$(git rev-parse --short HEAD)",
    "fix": "pretty-quick --staged && tslint --fix -p .",
    "riffraff": "VERBOSE=true ARTEFACT_PATH=$(pwd) riffraff-artefact",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn fix && GIT_DIR=../.git && yarn type-check && yarn bundle-check-client-if-changed"
    }
  },
  "cloudformation": "../cfn.yaml",
  "riffraffFile": "../riff-raff.yaml",
  "uploadArtefact": true,
  "buildDir": "dist",
  "projectName": "support:manage-frontend",
  "main": "./dist/server",
  "browserslist": [
    ">0.25%",
    "Chrome >= 49",
    "ChromeAndroid >= 49",
    "Safari >= 8",
    "ios_saf >= 9",
    "IE >= 11",
    "Edge >= 13",
    "Firefox >= 46",
    "FirefoxAndroid >= 46",
    "Samsung >= 4",
    "Opera >= 54",
    "OperaMini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@testing-library/react": "^8.0.1",
    "@types/assets-webpack-plugin": "^3.5.1",
    "@types/base-64": "^0.1.2",
    "@types/bunyan": "^1.8.4",
    "@types/color": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/csurf": "^1.9.35",
    "@types/express": "^4.11.1",
    "@types/google.analytics": "^0.0.39",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.136",
    "@types/node": "^10.1.2",
    "@types/node-fetch": "^1.6.9",
    "@types/number-to-words": "^1.2.0",
    "@types/prettier": "^1.12.4",
    "@types/raven": "^2.5.1",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.8.23",
    "@types/react-daterange-picker": "^2.0.1",
    "@types/react-dom": "^16.8.4",
    "@types/react-stripe-elements": "^1.3.2",
    "@types/react-test-renderer": "^16.8.2",
    "@types/url-parse": "^1.4.2",
